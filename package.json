{
  "name": "sveltekit-starter",
  "version": "0.0.2",
  "description": "Opinionated starter template for SvelteKit + Vite + TypeScript + ESLint + SCSS + TailwindCSS + Stylelint",
  "author": "Patrick Rupp",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "dev": "cross-env NODE_ENV=dev vite dev --config ./conf/vite.config.mjs",
    "build": "cross-env NODE_ENV=prod vite build --config ./conf/vite.config.mjs",
    "preview": "cross-env NODE_ENV=prod vite preview --config ./conf/vite.config.mjs",
    "type-check": "yarn tsc && yarn svelte-check",
    "lint": "yarn lint:js && yarn lint:css",
    "lint:css": "stylelint ./src/**/*.{css,scss,html,svelte} --config ./conf/stylelint.config.cjs --fix",
    "lint:js": "eslint . --config ./conf/eslint.config.cjs --ignore-path .gitignore --fix"
  },
  "devDependencies": {
    "@modyqyw/vite-plugin-eslint": "3.1.0",
    "@sveltejs/adapter-node": "1.0.0",
    "@sveltejs/kit": "1.0.1",
    "@types/eslint": "8.4.10",
    "@typescript-eslint/eslint-plugin": "5.47.0",
    "@typescript-eslint/parser": "5.47.0",
    "autoprefixer": "10.4.13",
    "cross-env": "7.0.3",
    "cssnano": "5.1.14",
    "dotenv": "16.0.3",
    "eslint": "8.30.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-airbnb-typescript": "17.0.0",
    "eslint-import-resolver-typescript": "3.5.2",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-import-alias": "1.2.0",
    "eslint-plugin-svelte": "2.14.1",
    "postcss": "8.4.20",
    "postcss-html": "1.5.0",
    "postcss-load-config": "4.0.1",
    "sass": "1.57.0",
    "stylelint": "14.16.0",
    "stylelint-config-recommended-scss": "8.0.0",
    "svelte": "3.55.0",
    "svelte-check": "2.10.2",
    "svelte-eslint-parser": "0.22.2",
    "svelte-preprocess": "5.0.0",
    "tailwindcss": "3.2.4",
    "tslib": "2.4.1",
    "typescript": "4.9.4",
    "vite": "4.0.2",
    "vite-plugin-stylelint": "4.1.3"
  },
  "peerDependencies": {
    "@sveltejs/kit": "1.0.1",
    "svelte": "3.55.0"
  }
}
