{
  "name": "sveltekit-starter",
  "version": "0.0.1",
  "description": "Opinionated starter template for SvelteKit + Vite + TypeScript + ESLint + SCSS + TailwindCSS + Stylelint",
  "author": "Patrick Rupp",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "dev": "cross-env NODE_ENV=dev vite dev --config ./conf/vite.config.mjs",
    "build": "cross-env NODE_ENV=prod vite build --config ./conf/vite.config.mjs",
    "preview": "cross-env NODE_ENV=prod vite preview --config ./conf/vite.config.mjs",
    "type-check": "yarn tsc && yarn svelte-check",
    "lint": "yarn lint:js && yarn lint:css",
    "lint:css": "stylelint ./src/**/*.{css,scss,html,svelte} --config ./conf/stylelint.config.cjs --fix",
    "lint:js": "eslint . --config ./conf/eslint.config.cjs --ignore-path .gitignore --fix"
  },
  "devDependencies": {
    "@sveltejs/adapter-node": "1.0.0-next.100",
    "@sveltejs/kit": "1.0.0-next.543",
    "@typescript-eslint/eslint-plugin": "5.42.1",
    "@typescript-eslint/parser": "5.42.1",
    "autoprefixer": "10.4.13",
    "cross-env": "7.0.3",
    "cssnano": "5.1.14",
    "dotenv": "16.0.3",
    "eslint": "8.27.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-airbnb-typescript": "17.0.0",
    "eslint-import-resolver-typescript": "3.5.2",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-import-alias": "1.2.0",
    "eslint-plugin-svelte": "2.12.0",
    "postcss": "8.4.19",
    "postcss-html": "1.5.0",
    "postcss-load-config": "4.0.1",
    "sass": "1.56.1",
    "stylelint": "14.14.1",
    "stylelint-config-recommended-scss": "8.0.0",
    "svelte": "3.53.1",
    "svelte-check": "2.9.2",
    "svelte-eslint-parser": "0.20.0",
    "svelte-preprocess": "4.10.7",
    "tailwindcss": "3.2.4",
    "tslib": "2.4.1",
    "typescript": "4.8.4",
    "vite": "3.2.3",
    "vite-plugin-eslint": "1.8.1",
    "vite-plugin-stylelint": "3.0.8"
  },
  "peerDependencies": {
    "@sveltejs/kit": "1.0.0-next.543",
    "svelte": "3.53.1"
  }
}
