{
  "css.customData": [
    ".vscode/css-custom-data.json"
  ],
  "css.lint.unknownAtRules": "ignore",
  "eslint.options": {
    "overrideConfigFile": "./conf/eslint.config.cjs"
  },
  "eslint.validate": [
    "javascript",
    "svelte"
  ],
  "search.exclude": {
    "yarn.lock": true
  },
  "stylelint.configFile": "./conf/stylelint.config.cjs",
  "stylelint.validate": [
    "css",
    "scss"
  ],
  "tailwindCSS.experimental.configFile": "./conf/tailwind.config.cjs",
  "tailwindCSS.emmetCompletions": true,
  "[svelte]": {
    "editor.defaultFormatter": "svelte.svelte-vscode"
  }
}
